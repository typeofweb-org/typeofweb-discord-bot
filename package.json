{
  "name": "typeofweb-discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/src/index.js\"",
    "eslint": "eslint --fix --cache",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "lint": "yarn tsc && yarn eslint",
    "test": "cross-env NODE_ENV=test yarn lint && yarn test:unit",
    "test:ci": "cross-env NODE_ENV=test yarn lint && yarn coverage",
    "test:unit": "cross-env NODE_ENV=test yarn mocha --file test/before-each.spec.ts 'src/**/*.spec.ts'",
    "coverage": "nyc yarn test:unit",
    "mocha": "mocha",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc && rsync -av --exclude='*.ts' src/** dist/src/",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Micha≈Ç Miszczyszyn <michal@mmiszy.pl> (https://typeofweb.com/)",
  "license": "AGPL",
  "dependencies": {
    "@discordjs/rest": "1.1.0",
    "@types/bluebird": "3.5.36",
    "algoliasearch": "4.14.2",
    "bluebird": "3.7.2",
    "bufferutil": "4.0.6",
    "discord.js": "14.3.0",
    "dotenv": "16.0.2",
    "isolated-vm": "4.4.1",
    "mongodb": "4.9.1",
    "monitorss": "6.14.11",
    "natural": "5.2.3",
    "node-cache": "5.1.2",
    "node-fetch": "2.6.1",
    "openai": "2.0.2",
    "polish-plurals": "1.1.0",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "secure-json-parse": "2.5.0",
    "typescript": "4.8.2",
    "zlib-sync": "0.1.7"
  },
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/dotenv": "8.2.0",
    "@types/mocha": "9.1.1",
    "@types/natural": "5.1.1",
    "@types/nock": "11.1.0",
    "@types/node": "18.7.14",
    "@types/node-fetch": "2.6.2",
    "@types/secure-json-parse": "1.0.3",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.8",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "cross-env": "7.0.3",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-functional": "4.2.2",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mocha": "10.0.0",
    "nock": "13.2.9",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "tsc-watch": "5.0.3"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn prettier --write"
    ],
    "**/*.{md,js,json}": [
      "yarn prettier --write"
    ]
  },
  "engines": {
    "node": "^18.0.0"
  }
}
